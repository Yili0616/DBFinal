<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ManageStation">
   <children>
      <Button fx:id="_createStation" layoutX="53.0" layoutY="314.0" mnemonicParsing="false" onAction="#createStationOnAction" text="Create Station" />
      <Button fx:id="_viewStation" layoutX="471.0" layoutY="314.0" mnemonicParsing="false" text="View Station" onAction="#viewStationOnAction"/>
      <TableView layoutX="130.0" layoutY="66.0" prefHeight="200.0" prefWidth="341.0">
        <columns>
          <TableColumn prefWidth="102.5" text="Station Name" />
          <TableColumn prefWidth="76.5" text="Stop ID" />
            <TableColumn prefWidth="55.0" text="Fare" />
            <TableColumn prefWidth="106.0" text="Status" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
